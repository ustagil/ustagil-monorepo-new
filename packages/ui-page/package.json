{
  "name": "@ustagil/ui-page",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn build:swc -w",
    "build": "rimraf dist && yarn build:swc && yarn types",
    "build:swc": "swc src -d dist",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rimraf ./dist .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.0 || ^18.0.0-0",
    "react-dom": "^17.0.0 || ^18.0.0-0"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.12",
    "@ustagil/ui-atom": "workspace:*",
    "@ustagil/ui-molecule": "workspace:*",
    "@ustagil/ui-organism": "workspace:*",
    "@ustagil/ui-template": "workspace:*",
    "@ustagil/utils": "workspace:*",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.10",
    "@types/react": "latest",
    "chokidar": "^3.5.3",
    "eslint": "^8.16.0",
    "eslint-config-ustagil": "workspace:*",
    "typescript": "^4.5.5"
  }
}
