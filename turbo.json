{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "inputs": [
                "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "package.json"
            ],
            "outputs": [
                "dist/**",
                "storybook-static/**"
            ],
            "dependsOn": [
                "^build"
            ],
            "outputMode": "new-only"
        },
        "start": {
            "inputs": [
                "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "dist/**",
                ".next/**",
                "package.json"
            ],
            "outputMode": "new-only"
        },
        "clean": {
            "outputMode": "errors-only"
        },
        "clean:rollup:cache": {
            "cache": false,
            "outputMode": "errors-only"
        },
        "coverage": {
            "inputs": [
                "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "package.json"
            ],
            "outputs": [
                "coverage/**"
            ],
            "dependsOn": [
                "^build",
                "^coverage"
            ],
            "outputMode": "new-only"
        },
        "format": {
            "inputs": [
                "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}"
            ],
            "outputMode": "errors-only"
        },
        "lint": {
            "inputs": [
                "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}"
            ],
            "outputMode": "errors-only"
        },
        "test": {
            "inputs": [
                "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
                "package.json"
            ],
            "dependsOn": [
                "^build",
                "^test"
            ],
            "outputMode": "errors-only"
        }
    }
}